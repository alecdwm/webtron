{
  "env": { "es6": true, "browser": true },
  "globals": { "global": "readonly", "process": "readonly" },
  "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
  "rules": {
    "eqeqeq": [2, "allow-null"],
    "no-console": [1, { "allow": ["log", "info", "warn", "error"] }],
    "no-debugger": 1,
    "no-unreachable": 1,
    "no-unused-vars": 1,
    "react-hooks/exhaustive-deps": "warn",
    "react-hooks/rules-of-hooks": "error",
    "react/prop-types": 0
  },
  "parser": "babel-eslint",
  "parserOptions": { "sourceType": "module" },
  "plugins": ["react", "react-hooks"],
  "settings": { "react": { "version": "16.8.6" } }
}
