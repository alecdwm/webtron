{
  "name": "webtron",
  "license": "MIT",
  "scripts": {
    "dev": "NEXT_TELEMETRY_DISABLED=1 next dev --port 3001",
    "fmt": "prettier --parser json --write .eslintrc .csscombrc .importsortrc; prettier --write next.config.js src; node node_modules/csscomb/src/cli.js -c .csscombrc src",
    "prebuild": "NEXT_TELEMETRY_DISABLED=1 NODE_ENV=production next build",
    "build": "NEXT_TELEMETRY_DISABLED=1 NODE_ENV=production next export -o dist",
    "postbuild": "touch dist/.gitkeep"
  },
  "dependencies": {
    "babel-eslint": "^10.1.0",
    "csscomb": "csscomb/csscomb.js#1f27f9e4e0f6a9f0849b6fc6e8325baf9371aeaf",
    "dayjs": "^1.8.25",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "import-sort-style-module": "^6.0.0",
    "next": "^9.3.4",
    "next-compose-plugins": "^2.2.0",
    "prettier": "^2.0.2",
    "prettier-loader": "^3.3.0",
    "prettier-plugin-import-sort": "^0.0.4",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.42.1"
  }
}
