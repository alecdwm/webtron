{
  "name": "webtron-client",
  "license": "GPL-3.0-only",
  "scripts": {
    "dev": "parcel --port 3000 src/index.html",
    "fmt": "prettier --write --parser json .eslintrc .parcelrc .csscombrc .importsortrc; prettier --write --parser html 'src/**/*.html'; prettier --write --parser css 'src/**/*.css'; prettier --write --parser babel 'src/**/*.js'; node node_modules/csscomb/src/cli.js -c .csscombrc src",
    "build": "parcel build src/index.html",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@parcel/validator-eslint": "^2.0.0-alpha.3.2",
    "babel-eslint": "^10.1.0",
    "csscomb": "csscomb/csscomb.js#1f27f9e4e0f6a9f0849b6fc6e8325baf9371aeaf",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "import-sort-style-module": "^6.0.0",
    "parcel": "^2.0.0-alpha.3.2",
    "postcss-modules": "^1.5.0",
    "prettier": "^1.19.1",
    "prettier-plugin-import-sort": "^0.0.3",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  }
}
